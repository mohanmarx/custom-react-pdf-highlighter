{
    "presets": [
        "@babel/preset-flow",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-react-jsx",
            {
                "throwIfNamespace": false, // defaults to true
                "runtime": "automatic" // defaults to classic
            }
        ],
        "@babel/plugin-proposal-class-properties"
    ]
}