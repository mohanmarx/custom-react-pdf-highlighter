{
  "name": "react-custom-pdf-highlighter",
  "version": "0.0.2-alpha4",
  "description": "Set of React components for PDF annotation",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run watch",
    "build": "NODE_ENV=production babel src -d lib --copy-files",
    "watch": "NODE_ENV=development babel --watch src -d lib --copy-files",
    "copy-flow": "flow-copy-source src lin",
    "clean": "rm -rf lib lin",
    "prepublishOnly": "npm run build && npm run copy-flow"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "lodash": "^4.17.20",
    "pdfjs-dist": "2.5.207",
    "react-new-window": "^0.1.2",
    "react-pointable": "^1.1.3",
    "react-rnd": "^10.2.4",
    "url-search-params": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=17.0",
    "react-dom": ">=17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "flow-bin": "0.138.0",
    "flow-copy-source": "^2.0.9",
    "gh-pages": "3.1.0",
    "husky": "4.3.4",
    "nwb": "^0.25.2",
    "prettier": "^2.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "author": "marx",
  "license": "MIT",
  "keywords": [
    "custom-react-pdf-highlighter",
    "react-pdf-highlighter",
    "pdf-highlighter",
    "pdf-highlight",
    "pdf",
    "highlight",
    "annotator",
    "react-component"
  ],
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohanmarx/react-custom-pdf-highlighter.git"
  },
  "bugs": {
    "url": "https://github.com/mohanmarx/react-custom-pdf-highlighter/issues"
  },
  "homepage": "https://mohanmarx.github.io/react-custom-pdf-highlighter"
}
