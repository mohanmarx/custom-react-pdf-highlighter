{
  "name": "custom-react-pdf-highlight",
  "version": "0.0.1-alpha-4-1.71",
  "description": "Set of React components for PDF annotation",
  "main": "lib/index.js",
  "scripts": {
    "build": "nwb build-react-component",
    "copy-flow": "flow-copy-source src lib",
    "copy-styles": "cp -r src/style lib",
    "copy-files": "npm run copy-flow && npm run copy-styles",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo",
    "test": "npm run flow",
    "flow": "flow",
    "precommit": "npm run flow",
    "deploy": "npm run build && gh-pages --dist ./demo/dist --branch gh-pages",
    "prepublishOnly": "npm run build && npm run copy-files",
    "prettier": "prettier --write 'src/**/*.js' 'demo/src/**/*.js'"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "lodash": "^4.17.15",
    "pdfjs-dist": "2.3.200",
    "react-new-window": "^0.1.2",
    "react-pointable": "^1.1.3",
    "react-rnd": "^10.1.9",
    "url-search-params": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=16.13",
    "react-dom": ">=16.13"
  },
  "devDependencies": {
    "flow-bin": "0.123.0",
    "flow-copy-source": "^2.0.9",
    "gh-pages": "2.2.0",
    "husky": "4.2.5",
    "nwb": "^0.24.5",
    "prettier": "^2.0.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "author": "marx",
  "license": "MIT",
  "keywords": [
    "custom-react-pdf-highlighter",
    "react-pdf-highlighter",
    "pdf-highlighter",
    "pdf-highlight",
    "pdf",
    "highlight",
    "annotator",
    "react-component"
  ],
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohanmarx/custom-react-pdf-highlight.git"
  },
  "bugs": {
    "url": "https://github.com/mohanmarx/custom-react-pdf-highlight/issues"
  },
  "homepage": "https://github.com/mohanmarx/custom-react-pdf-highlight#readme"
}
