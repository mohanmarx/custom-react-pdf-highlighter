{
  "name": "custom-react-pdf-highlight",
  "version": "0.0.1-alpha-4-1.72",
  "description": "Set of React components for PDF annotation",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run watch",
    "build": "NODE_ENV=production babel src -d build --copy-files",
    "watch": "NODE_ENV=development babel --watch src -d build --copy-files",
    "copy-flow": "flow-copy-source src build",
    "clean": "rm -rf build",
    "prepublishOnly": "npm run build && npm run copy-flow"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "lodash": "^4.17.15",
    "pdfjs-dist": "2.3.200",
    "react-new-window": "^0.1.2",
    "react-pointable": "^1.1.3",
    "react-rnd": "^10.1.9",
    "url-search-params": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=16.13",
    "react-dom": ">=16.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "flow-copy-source": "^2.0.9"
  },
  "author": "marx",
  "license": "MIT",
  "keywords": [
    "custom-react-pdf-highlighter",
    "react-pdf-highlighter",
    "pdf-highlighter",
    "pdf-highlight",
    "pdf",
    "highlight",
    "annotator",
    "react-component"
  ],
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohanmarx/custom-react-pdf-highlight.git"
  },
  "bugs": {
    "url": "https://github.com/mohanmarx/custom-react-pdf-highlight/issues"
  },
  "homepage": "https://github.com/mohanmarx/custom-react-pdf-highlight#readme"
}
